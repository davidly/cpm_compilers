	TITLE LUNTB - APPLE CPM LUN TABLE

;	TTY I/O DRIVER
	.8080

	EXTRN	TTYDRV,PUNRDR,LPTDRV,DSKDRV

	ENTRY	$LUNTB

$LUNTB:	DB	11		;TEN LUN'S
	
	DW	TTYDRV		;1= TTY
	DW	LPTDRV		;2= PRINTER
	DW	TTYDRV		;3= TTY
	DW	PUNRDR		;4= PUNCH,READER
	DW	TTYDRV		;5= TTY
	DW	DSKDRV		;6= FORT06.DAT
	DW	DSKDRV		;7= FORT07.DAT
	DW	DSKDRV		;AND SO ON.
	DW	DSKDRV
	DW	DSKDRV
	END
