BEGIN
COMMENT THIS PROGRAM TESTS THE FUNCTIONING OF THE CONCATENATE
        AND STORE STRING INTERMEDIATE INSTRUCTIONS.;
STRING A,B,C;

READ(C);
A:=B:=C;
WRITE(A," ",B," ", C);
A:=B:=C||" TEST";
WRITE(A,B,C);
A:=B:="TEST"||C||" END";
WRITE(A,B,C);
WRITE(A||B);
WRITE(A||"TEST");
IF (A:="HELLO")>= "HELLA" THEN WRITE(A);
END
EOF
